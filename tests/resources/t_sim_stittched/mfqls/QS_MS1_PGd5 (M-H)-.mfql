##########################################################
# Identify PG with checking the precursor mass    #
##########################################################

QUERYNAME = PGd5;
	DEFINE pr = 'C[37..45] H[65..75] D[5] O[10] P[1]' WITH DBR = (2.5,7), CHG = -1;

IDENTIFY
	pr IN MS1-

SUCHTHAT
	isOdd(pr.chemsc[H]) AND
	isOdd(pr.chemsc[db]*2)
  
REPORT  
	PRM = pr.mass;
	EC = pr.chemsc;
	CLASS = "PGd5" % "()";
	PRC = "%d" % "((pr.chemsc)[C] - 6)";
	PRDB = "%d" % "((pr.chemsc)[db] - 2.5)";
	PROH = "%d" % "((pr.chemsc)[O] - 10)";
	SPECIE = "PGd5 %d:%d:%d" % "((pr.chemsc)[C]-6, pr.chemsc[db]-2.5, pr.chemsc[O]-10)";
	POLARITY = "-";
	TYPE = "M-H";

      QSERR = "%2.2f" % "(pr.errppm)";
	QS = pr.intensity;
	;

################ end script ##################