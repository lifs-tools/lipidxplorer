##########################################################
# Identify LPG with checking the precursor mass    #
##########################################################

QUERYNAME = LPGd5;
	DEFINE pr = 'C[21..25] H[37..45] D[5] O[9] P[1]' WITH DBR = (1.5,7.5), CHG = -1;

IDENTIFY
	pr IN MS1- 

SUCHTHAT
	isOdd(pr.chemsc[H]) AND
	isOdd(pr.chemsc[db]*2)

REPORT 
	PRM = pr.mass;
	EC = pr.chemsc;
	CLASS = "LPGd5" % "()";
	PRC = "%d" % "((pr.chemsc)[C] - 6)";
	PRDB = "%d" % "((pr.chemsc)[db] - 1.5)";
	PROH = "%d" % "((pr.chemsc)[O] - 9)";
	SPECIE = "LPGd5 %d:%d:%d" % "((pr.chemsc)[C]-6, pr.chemsc[db]-1.5, pr.chemsc[O]-9)";
	POLARITY = "-";
	TYPE = "M-H";

      QSERR = "%2.2f" % "(pr.errppm)";
	QS = pr.intensity;
	;

################ end script ##################