##########################################################
# Identify PI with checking the precursor mass    #
##########################################################

QUERYNAME = PId5;
	DEFINE pr = 'C[40..48] H[69..79] D[5] O[13] P[1]' WITH DBR = (3.5,7.5), CHG = -1;

IDENTIFY
	pr IN MS1-

SUCHTHAT
	isOdd(pr.chemsc[H]) AND
	isOdd(pr.chemsc[db]*2)

REPORT 
	PRM = pr.mass;
	EC = pr.chemsc;
	CLASS = "PId5" % "()";
	PRC = "%d" % "((pr.chemsc)[C] - 9)";
	PRDB = "%d" % "((pr.chemsc)[db] - 3.5)";
	PROH = "%d" % "((pr.chemsc)[O] - 13)";
	SPECIE = "PId5 %d:%d:%d" % "((pr.chemsc)[C]-9, pr.chemsc[db]-3.5, pr.chemsc[O]-13)";
	POLARITY = "-";
	TYPE = "M-H";

      QSERR = "%2.2f" % "(pr.errppm)";
	QS = pr.intensity;
	;
 
################ end script ##################