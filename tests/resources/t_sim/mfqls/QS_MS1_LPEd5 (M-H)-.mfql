##########################################################
# Identify LPE with checking the precursor mass    #
##########################################################

QUERYNAME = LPEd5;
	DEFINE pr = 'C[20..24] H[36..44] D[5] N[1] O[7] P[1]' WITH DBR = (1.5,1.5), CHG = -1;

IDENTIFY
	pr IN MS1-

SUCHTHAT
	isEven(pr.chemsc[H]) AND
	isOdd(pr.chemsc[db] * 2)
  
REPORT 
	PRM = pr.mass;
	EC = pr.chemsc;
	CLASS = "LPEd5" % "()";
	PRC = "%d" % "((pr.chemsc)[C] - 5)";
	PRDB = "%d" % "((pr.chemsc)[db] - 1.5)";
	PROH = "%d" % "((pr.chemsc)[O] - 7)";
	SPECIE = "LPEd5 %d:%d:%d" % "((pr.chemsc)[C]-5, pr.chemsc[db]-1.5, pr.chemsc[O]-7)";
	POLARITY = "-";
	TYPE = "M-H";

      QSERR = "%2.2f" % "(pr.errppm)";
	QS = pr.intensity;
	;

################ end script ##################