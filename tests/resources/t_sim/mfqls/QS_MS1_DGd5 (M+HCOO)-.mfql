##########################################################
# Identify DGs with checking the precursor mass     #
##########################################################

QUERYNAME = DGd5;
	DEFINE pr = 'C[35..43] H[60..70] D[5] O[7]' WITH DBR = (2.5,15.5), CHG = -1;

IDENTIFY
	pr IN MS1-

SUCHTHAT
      isEven(pr.chemsc[H]) AND 
	isOdd(pr.chemsc[C])

REPORT  
	PRM = pr.mass;
	EC = pr.chemsc;
	CLASS = "DGd5" % "()";
	PRC = "%d" % "((pr.chemsc)[C] - 4)";
	PRDB = "%d" % "((pr.chemsc)[db] - 2.5)";
	PROH = "%d" % "((pr.chemsc)[O] - 7)";
	SPECIE = "DGd5 %d:%d:%d" % "((pr.chemsc)[C]-4, pr.chemsc[db]-2.5, pr.chemsc[O]-7)";
	POLARITY = "-";
	TYPE = "M+HCOO";

      QSERR = "%2.2f" % "(pr.errppm)";
	QS = pr.intensity;
	;

################ end script ##################