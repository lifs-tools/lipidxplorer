# Screening for SM - MSMS Check for 184# written 2017 - 01 - 30# Lars F. Eggers
# modified DS 180607
QUERYNAME =SphingomyelineOne;
DEFINE PR = 'C[31..49] H[67..103] O[6..7] N[2] P[1]' WITH DBR = (0.5,2.5), CHG = +1;DEFINE FR = 'C[5] H[15] O[4] N[1] P[1]' WITH DBR = (1.5,3.5), CHG = +1;



IDENTIFY
	
	# marking
	PR IN MS1+ AND	FR IN MS2+


SUCHTHAT
	isOdd(PR.chemsc[C]) 
	AND (avg(FR.intensity)/avg(PR.intensity) >=1 AND avg(FR.intensity)/avg(PR.intensity) <=100)
  
REPORT 
	SPECIE = "SM %d:%d;%d" % "((PR.chemsc)[C] - 5, (PR.chemsc)[db] - 0.5, (PR.chemsc)[O] - 6)";
   	CLASS = "SM";	IDMSLEVEL = 2;	QUANTMSLEVEL = 1;	
	MASS = PR.mass;
	ISOBARIC = PR.isobaric;
	CHEMSC = PR.chemsc;
	ERRppm = "%2.2f" % "(PR.errppm)";	FRERRppm = "%2.2f" % "(FR.errppm)";	INT = PR.intensity;	QUALA = FR.intensity;	QUALB = FR.intensity;	QUALC = FR.intensity;	;		
	

################ end script ##################
