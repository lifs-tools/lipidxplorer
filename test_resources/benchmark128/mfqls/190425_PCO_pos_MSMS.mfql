# Screening for PCO - MS/MS# Check for Fragment 184# written 2017 - 02 - 13# Lars F. Eggers
QUERYNAME = PhosphatidylcholineEther;
DEFINE PR = 'C[32..56] H[67..115] O[7] N[1] P[1]' WITH DBR = (0.5,10.5), CHG = +1;DEFINE FR = 'C[5] H[15] O[4] N[1] P[1]' WITH DBR = (1.5,3.5), CHG = +1;



IDENTIFY
	
	# marking
	PR IN MS1+ AND	FR IN MS2+

SUCHTHAT
	isEven(PR.chemsc[C])
  
REPORT 
	SPECIE = "PC O-%d:%d" % "((PR.chemsc)[C] - 8, (PR.chemsc)[db] - 0.5)";  
	CLASS = "PC";	MASS = PR.mass;	IDMSLEVEL = 2;	QUANTMSLEVEL = 1;		ISOBARIC = PR.isobaric;	CHEMSC = PR.chemsc;	ERRppm = "%2.2f" % "(PR.errppm)";	FRERRppm = "%2.2f" % "(FR.errppm)";	INT = PR.intensity;	QUALA = FR.intensity;	QUALB = FR.intensity;	QUALC = FR.intensity;	;
	

################ end script ##################
