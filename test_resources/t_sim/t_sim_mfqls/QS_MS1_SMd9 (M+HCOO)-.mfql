##########################################################
# Identify SM with checking the precursor mass    #
##########################################################

QUERYNAME = SMd9;
	DEFINE pr = 'C[40..48] H[69..85] D[9] N[2] O[8] P[1]' WITH DBR = (1.5,3.5), CHG = -1;

IDENTIFY  
	pr IN MS1-

SUCHTHAT
	isOdd(pr.chemsc[H]) AND
	isOdd(pr.chemsc[db] * 2)
  
REPORT 
	PRM = pr.mass;
	EC = pr.chemsc;
	CLASS = "SMd9" % "()";	
	PRC = "%d" % "((pr.chemsc)[C] - 6)";
	PRDB = "%d" % "((pr.chemsc)[db] - 1.5)";
	PROH = "%d" % "((pr.chemsc)[O] - 8)";
	SPECIE = "SMd9 %d:%d:%d" % "((pr.chemsc)[C]-6, pr.chemsc[db]-1.5, pr.chemsc[O]-8)";
	POLARITY = "-";
	TYPE = "M+HCOO";

      QSERR = "%2.2f" % "(pr.errppm)";
	QS = pr.intensity;
	;

################ end script ##################
