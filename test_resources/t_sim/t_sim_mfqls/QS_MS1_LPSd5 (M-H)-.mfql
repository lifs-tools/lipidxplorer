##########################################################
# Identify LPS with checking the precursor mass    #
##########################################################

QUERYNAME = LPSd5;
	DEFINE pr = 'C[21..25] H[36..44] D[5] N[1] O[9] P[1]' WITH DBR = (2.5,9), CHG = -1;

IDENTIFY
	pr IN MS1-

SUCHTHAT
	isEven(pr.chemsc[H]) AND
	isOdd(pr.chemsc[db]*2)
  
REPORT 
	PRM = pr.mass;
	EC = pr.chemsc;
	CLASS = "LPSd5" % "()";
	PRC = "%d" % "((pr.chemsc)[C] - 6)";
	PRDB = "%d" % "((pr.chemsc)[db] - 2.5)";
	PROH = "%d" % "((pr.chemsc)[O] - 9)";
	SPECIE = "LPSd5 %d:%d:%d" % "((pr.chemsc)[C]-6, pr.chemsc[db]-2.5, pr.chemsc[O]-9)";
	POLARITY = "-";
	TYPE = "M-H";

      QSERR = "%2.2f" % "(pr.errppm)";
	QS = pr.intensity;
	;

################ end script ##################