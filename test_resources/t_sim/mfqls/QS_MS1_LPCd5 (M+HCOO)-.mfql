##########################################################
# Identify LPC with checking the precursor mass    #
##########################################################

QUERYNAME = LPCd5;
	DEFINE pr = 'C[24..28] H[44..52] D[5] N[1] O[9] P[1]' WITH DBR = (1.5,2.5), CHG = -1;

IDENTIFY	
	pr IN MS1-

SUCHTHAT
	isEven(pr.chemsc[H]) AND
	isOdd(pr.chemsc[db]*2)

REPORT 
	PRM = pr.mass;
	EC = pr.chemsc;
	CLASS = "LPCd5" % "()";
	PRC = "%d" % "((pr.chemsc)[C] - 9)";
	PRDB = "%d" % "((pr.chemsc)[db] - 1.5)";
	PROH = "%d" % "((pr.chemsc)[O] - 9)";
	SPECIE = "LPCd5 %d:%d:%d" % "((pr.chemsc)[C]-9, pr.chemsc[db]-1.5, pr.chemsc[O]-9)";
	POLARITY = "-";
	TYPE = "M+HCOO";

      QSERR = "%2.2f" % "(pr.errppm)";
	QS = pr.intensity;
	;

################ end script ##################