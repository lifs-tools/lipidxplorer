##########################################################
# Identify PS with checking the precursor mass    #
##########################################################

QUERYNAME = PSd5;
	DEFINE pr = 'C[37..45] H[64..74] D[5] N[1] O[10] P[1]' WITH DBR = (3.5,15.5), CHG = -1;

IDENTIFY  
	pr IN MS1-

SUCHTHAT
	isEven(pr.chemsc[H]) AND
	isOdd(pr.chemsc[db]*2)
  
REPORT 
	PRM = pr.mass;
	EC = pr.chemsc;
	CLASS = "PSd5" % "()";
	PRC = "%d" % "((pr.chemsc)[C] - 6)";
	PRDB = "%d" % "((pr.chemsc)[db] - 3.5)";
	PROH = "%d" % "((pr.chemsc)[O] - 10)";
	SPECIE = "PSd5 %d:%d:%d" % "((pr.chemsc)[C]-6, pr.chemsc[db]-3.5, pr.chemsc[O]-10)";
	POLARITY = "-";
	TYPE = "M-H";

      QSERR = "%2.2f" % "(pr.errppm)";
	QS = pr.intensity;
	;

################ end script ##################
