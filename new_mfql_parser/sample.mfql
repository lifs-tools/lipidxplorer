########################################################### Identify PC SPCCIES ###########################################################QUERYNAME = PCFAS;	DEFINE pr = 'C[30..80] H[40..300] O[10] N[1] P[1]' WITH DBR = (2.5,14.5), CHG = -1;	DEFINE FA1 = 'C[10..40] H[20..100] O[2]' WITH DBR = (1.5,7.5), CHG = -1;	DEFINE FA2 ='C[10..40] H[20..100] O[2]' WITH DBR = (1.5,7.5), CHG = -1;IDENTIFY	pr IN MS1- AND	FA1 IN MS2- AND	FA2 IN MS2-SUCHTHAT	isOdd(pr.chemsc[H]) AND	isOdd(pr.chemsc[db]*2) AND	FA1.chemsc + FA2.chemsc + 'C9 H19 N1 O6 P1' == pr.chemscREPORT	PRM = pr.mass;	EC = pr.chemsc;	CLASS = "PC" % "()";	PRC = "%d" % "((pr.chemsc)[C] - 9)";	PRDB = "%d" % "((pr.chemsc)[db] - 2.5)";	PROH = "%d" % "((pr.chemsc)[O] - 10)";	SPECIE = "PC %d:%d:%d" % "((pr.chemsc)[C]-9, pr.chemsc[db] - 2.5, pr.chemsc[O]-10)";    PRERR = "%2.2f" % "(pr.errppm)";	PRI = pr.intensity;	FA1M = FA2.mass;	FA1C = "%d" % "((FA2.chemsc)[C])";	FA1DB = "%d" % "((FA2.chemsc)[db] - 1.5)";        FA1ERR = "%2.2f" % "(FA2.errppm)";	FA1I = FA2.intensity;	FA2M = FA1.mass;	FA2C = "%d" % "((FA1.chemsc)[C])";	FA2DB = "%d" % "((FA1.chemsc)[db] - 1.5)";    FA2ERR = "%2.2f" % "(FA1.errppm)";	FA2I = FA1.intensity;	;################ end script ##################